DATABASE_URL="postgresql://{POSTGRES_USERNAME}:{POSTGRES_PASSWORD}@{DB_HOST}:{DB_PORT}/{DB_NAME}?schema=public"
PORT= # The port for the NestJS server to attach to (defaults to 3000 if left blank)

JWT_SECRET= # JWT secret used by the signer to generate your secure token